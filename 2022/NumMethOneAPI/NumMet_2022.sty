    \pdfbookmark[1]{Приложение \theappendixcnt. #1}{bkm-pril\arabic{appendixcnt}}
    \begin{center}\normalsize
        \textbf{Приложение \arabic{appendixcnt}}\\[1mm]
        \textbf{#1}\\[3mm]
    \end{center}
}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
\newcounter{cntAlg}
\newcounter{cntStrAlg}[cntAlg]
%
\newlength{\algCaptUpVSkip}
\newlength{\algCaptDnVSkip}
\newlength{\algCaptHSkip}
\newlength{\algCaptAlg}
\newlength{\algStringNumWidth}
\newlength{\algStringAfterNumOffs}
\newlength{\algTabulator}
\newlength{\textAwidth}
%
\setlength\algCaptUpVSkip{2mm}
\setlength\algCaptDnVSkip{1.3mm}
\setlength\algCaptHSkip{1mm}
\setlength\algStringAfterNumOffs{4mm}
\settowidth{\algStringNumWidth}{\mbox{\small 999:\hspace*{\algStringAfterNumOffs}}}
\settowidth{\algTabulator}{\mbox{nn}}
\setlength{\textAwidth}{\textwidth}
%
\DeclareDocumentCommand{\algLine}{O{0} O{} m}{%
    \setlength{\textAwidth}{\textwidth}
    \noindent\parbox{\algStringNumWidth}{\refstepcounter{cntStrAlg}%
        \hfill\small\thecntStrAlg:\hspace*{\algStringAfterNumOffs}%
        \IfNoValueTF{#2}%
            {}%
            {\label{#2}}%
	}%parbox
        \IfNoValueTF{#1}%
            {}%
            {\hspace*{\dimexpr(#1\algTabulator)\relax}}%
	\parbox[t]{\dimexpr(\textAwidth-#1\algTabulator-\algStringNumWidth)\relax}{#3}%
}
%
\DeclareDocumentCommand{\algLineDouble}{O{0} O{} m m}{%
    \setlength{\textAwidth}{82.5mm}
    \noindent\parbox{\algStringNumWidth}{\refstepcounter{cntStrAlg}%
       \hfill\small\thecntStrAlg:\hspace*{\algStringAfterNumOffs}%
        \IfNoValueTF{#2}%
            {}%
            {\label{#2}}%
	}%parbox
        \IfNoValueTF{#1}%
            {}%
            {\hspace*{\dimexpr(#1\algTabulator)\relax}}%
	\parbox[t]{\dimexpr(\textAwidth-#1\algTabulator-\algStringNumWidth)\relax}{#3\phantom{!}}%
	\hfill
	\parbox{\algStringNumWidth}{\hfill\small\thecntStrAlg:\hspace*{\algStringAfterNumOffs}%
        \IfNoValueTF{#2}%
        {}%
        {\label{#2}}%
    }%parbox
        \IfNoValueTF{#1}%
            {}%
            {\hspace*{\dimexpr(#1\algTabulator)\relax}}%
	\parbox[t]{\dimexpr(\textAwidth-#1\algTabulator-\algStringNumWidth)\relax}{#4\phantom{!}}%
    \setlength{\textAwidth}{\textwidth}
}
%
%
\newenvironment{nmAlgorithm}[3]{\small%
\noindent\refstepcounter{cntAlg}\label{#1}\hrule\par\vspace*{\algCaptUpVSkip}%
\settowidth{\algCaptAlg}{\hspace*{\algCaptHSkip}\text{Алгоритм~\thecntAlg.\ }}
\noindent\parbox[t]{\algCaptAlg}{\hfill\text{Алгоритм~\thecntAlg.\ }}%
~\parbox[t]{\dimexpr(\textwidth-\algCaptAlg)\relax}{\raggedright #2}\\[1mm]%
\noindent\parbox{\algCaptAlg}{\hfill\text{\oppo{Algorithm~\thecntAlg.\ }}}%
~\raisebox{6.7pt}{\parbox[t]{\dimexpr(\textwidth-\algCaptAlg)}{\raggedright \oppo{#3}}}\par%
\vspace*{\algCaptDnVSkip}\hrule\vspace*{\algCaptUpVSkip}\par
}{%
\par\vspace*{\algCaptDnVSkip}\hrule\vspace*{\algCaptUpVSkip}\par
}
%
\newcommand{\algLineComment}[1]{%
\noindent\centering #1}
%
\newcommand{\algLineDoubleComment}[2]{%
\noindent\parbox{82mm}{\centering #1}\rule[-3mm]{0pt}{6mm}\hfill\parbox{82mm}{\centering #2}}
